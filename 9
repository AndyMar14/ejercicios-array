using System;

public class Program
{
	public static void Main()
	{
		int mul = 0;
		while(mul != -1){
			Console.WriteLine("\nIngrese un nuemro");
			mul = int.Parse(Console.ReadLine());
			if(mul == -1){
				break;
			}else{
				for(int i = 0; i <= 12 ; i++){
				Console.WriteLine(mul+" X "+ i + " = " + mul * i);
				}
			}
		}
	}
}
