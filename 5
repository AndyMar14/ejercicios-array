using System;

public class Program
{
	public static void Main()
	{
		    string[] nombres = new string[100];
	
			
			for(int i = 0; i < 100; i++)
			{
				Console.Write("\nNombre: ");
				nombres[i] = Console.ReadLine();
			}
			
			for(int i = 0; i < 100; i++)
			{
				Console.Write("\nNombre "+ i +" "+nombres[i] );
			}

			
	
	}
}
