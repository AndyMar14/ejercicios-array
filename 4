using System;
using System.Linq;

public class Program
{
	public static void Main()
	{
		    int[] numeros = new int[10];
			int n; 
			int max = numeros[0];
			for(int i = 0; i < 10; i++)
			{
				Console.Write("Numero: ");
				n = int.Parse(Console.ReadLine());
				numeros[i] = n;
			}

			Console.WriteLine("El numero mayor es: " + numeros.Max());

	}
}
